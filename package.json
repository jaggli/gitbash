{
  "name": "gitbash",
  "version": "1.6.1",
  "description": "Opinionated git utilities for bash with fzf-powered menus",
  "author": "jaggli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaggli/gitbash.git"
  },
  "homepage": "https://github.com/jaggli/gitbash#readme",
  "bugs": {
    "url": "https://github.com/jaggli/gitbash/issues"
  },
  "keywords": [
    "bash",
    "git",
    "fzf",
    "shell",
    "utilities",
    "cli",
    "branch",
    "commit",
    "stash"
  ],
  "bin": {
    "gitbash": "bin/gitbash"
  },
  "files": [
    "bin/",
    "commands/",
    "doc/",
    "README.md",
    "LICENSE"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run version && VERSION=$(node -p \"require('./package.json').version\") && git tag \"v$VERSION\" && ./bin/gitbash commit -p \"chore: release v$VERSION\" && git push --tags && npm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8"
  }
}
